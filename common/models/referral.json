{
  "name": "Referral",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "Ownable": true
  },
  "properties": {
    "approved": {
      "type": "boolean",
      "default": false
    },
    "data": {
      "type": "object",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "form": {
      "type": "belongsTo",
      "model": "Form",
      "foreignKey": "formId",
      "options": {
        "nestRemoting": true
      }
    },
    "attachments": {
      "type": "hasMany",
      "model": "Attachment",
      "options": {
        "nestRemoting": true
      }
    },
    "signedDocument": {
      "type": "hasOne",
      "model": "Attachment",
      "foreignKey": "signedDocumentOfId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
