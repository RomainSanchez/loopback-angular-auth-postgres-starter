{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "memory",
    "public": false
  },
  "AccessToken": {
    "dataSource": "postgres",
    "public": false
  },
  "ACL": {
    "dataSource": "memory",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "postgres",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "postgres",
    "public": true
  },
  "Community": {
    "dataSource": "postgres",
    "public": true,
    "options": {
      "emailVerificationRequired": false,
      "ttl": 1800
    }
  },
  "Form": {
    "dataSource": "postgres",
    "public": true
  },
  "Referral": {
    "dataSource": "postgres",
    "public": true
  }
}
