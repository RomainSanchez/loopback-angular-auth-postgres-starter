{% extends 'base.twig' %}

{% block instructions %}
  <p>
  </p>
{% endblock %}

{% block content %}
  <div class="row">
    <p id="creation" class="title">{{ data.creation ? 'Création du DU' : 'Mise à jour du DU' }}</p>
  </div>

  <div class="row">
    <p class="label">Organisation de la collectivité:</p>
    <p id="organization" class="value">{{ data.organization }}</p>
  </div>

  <div class="row">
    <p class="label">DU rélisé par le CDG:</p>
    <p id="cdg" class="value">{{ data.cdg ? 'Oui': 'Non' }}</p>
  </div>

  <div class="row">
    <p class="label">Personnel consulté:</p>
    <p id="staff-consulted" class="value">{{ data.staffConsulted ? 'Oui': 'Non' }}</p>
  </div>
{% endblock %}
